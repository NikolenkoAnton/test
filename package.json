{
  "name": "admin-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "db-seed": "cross-env NODE_ENV=production sequelize db:seed:all",
    "db-seed:undo": "cross-env NODE_ENV=production sequelize db:seed:undo",
    "db-seed:undo:all": "cross-env NODE_ENV=production sequelize db:seed:undo:all",
    "db-seed:generate": "cross-env NODE_ENV=production ./node_modules/.bin/sequelize seed:generate --name ",
    "db-migration": "cross-env NODE_ENV=production sequelize db:migrate",
    "db-migration:generate": "cross-env NODE_ENV=production ./node_modules/.bin/sequelize migration:generate --name ",
    "db-migration:undo": "cross-env NODE_ENV=production sequelize db:migrate:undo",
    "db-migration:undo:all": "cross-env NODE_ENV=production sequelize db:migrate:undo:all",
    "mac": "rm -rf dist/* && tsc && NODE_ENV=local node --experimental-modules --es-module-specifier-resolution=node .",
    "win": "tsc && node --experimental-modules --es-module-specifier-resolution=node dist/src/index.js",
    "build": "tsc",
    "start": "node --experimental-modules --es-module-specifier-resolution=node dist/src/index.js",
    "start:dev": "nodemon src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "pretty": "prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.axiom.bet:sport/admin-api.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@sentry/types": "^7.58.1",
    "@types/express": "^4.17.14",
    "@types/lodash": "4.14.188",
    "@types/multer": "1.4.7",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "8.22.0",
    "prettier": "^2.7.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@sentry/node": "^6.16.1",
    "@types/node-cron": "^3.0.0",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "formidable": "^2.0.1",
    "js-big-decimal": "^2.0.4",
    "js-combinatorics": "^2.1.1",
    "lodash": "4.17.21",
    "multer": "1.4.5-lts.1",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.7.5",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "seedrandom": "^3.0.5",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.3.0",
    "sequelize-typescript": "^2.1.1",
    "string-to-stream": "3.0.1",
    "swagger-express-ts": "1.1.0",
    "swagger-ui-dist": "^4.1.2",
    "typedi": "^0.10.0",
    "xmlbuilder2": "3.0.2"
  }
